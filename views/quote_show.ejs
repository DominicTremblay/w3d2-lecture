<%- include('./partials/_head') %>

<div class="container">
    <h1>Edit Quote</h1>

    <!-- {quoteContent: movieQuotesDb[id]} -->
    <form method="POST" action="/quotes/<%= quoteContent.id %>">
    <div class="form-group">
      <div style="width: 20em;">
        <input
          style="width: 100%"
          type="text"
          name="quoteContent"
          id="quote"
          value="<%= quoteContent.quote %>">
      </div>
      

        <input type="submit" class="btn btn-primary" value="Update" />
      </div>
    </form>
    </div>
  </body>
</html>
